<Window x:Class="Inicio_1_1.DatsSevice"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="DatsSevice" Height="500" Width="500">

    <Grid RenderTransformOrigin="0.075,0.446">
       
        <Label Content="Nombre" HorizontalAlignment="Left" Margin="39,33,0,0" VerticalAlignment="Top"/>
        <Label Content="Telefono" HorizontalAlignment="Left" Margin="39,87,0,0" VerticalAlignment="Top"/>
        <Label Content="Direccion" HorizontalAlignment="Left" Margin="39,149,0,0" VerticalAlignment="Top"/>
        
        <TextBox  x:Name="TbName" HorizontalAlignment="Left" Height="23" Margin="136,33,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <TextBox  x:Name="TbTelephone" HorizontalAlignment="Left" Height="23" Margin="136,87,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <TextBox  x:Name="TbAddress" HorizontalAlignment="Left" Height="23" Margin="136,149,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        
        <Button Content="Aceptar" HorizontalAlignment="Left" Margin="329,175,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>

    </Grid>
</Window>
